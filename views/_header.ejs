<header class="main-csv-upload">
    <h1>CSV Upload</h1>
    <% if(locals.path){ %>
        <div>
            <ul id="nav">
                <% if(path == "visualizer") { %>
                    <li><a href="/csv/" style="text-decoration: none;">Return To CSV File List</a></li>
                    <li><a href="/" style="text-decoration: none;">Upload a New CSV File</a></li>
                    <% } %>
                        <% if(path == "files") { %>
                            <li><a href="/">Return To HomePage</a></li>
                            <% } %>
                                <% if(path == "home") { %>
                                    <li><a href="/csv">View My All CSV Files</a></li>
                                    <% } %>
            </ul>
        </div>
        <% } %>
</header>